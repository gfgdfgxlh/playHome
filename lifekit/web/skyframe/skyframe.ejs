<script> 

    var net;
    var _token = '<%=token %>';
    var _method = '<%=method %>';
    $(function(){ 
        ShowWaitting(true);

        $(".menu li:has(div)").eq(0).addClass("hover");
	    $(".scrollLoading").scrollLoading(); 

        net = NetClient('<%=host %>','<%=server %>','<%=token %>',function(){
            net.getView(_method, '<%-params %>',function(result,parms){
                ShowWaitting(false);
                $("#divAll").html(result);
            });
        });
    });

</script>

